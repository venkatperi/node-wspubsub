{
  "wspubsub": {
    "host": "localhost",
    "port": 10016,
    "path": "/",
    "channel": {
      "retain": 10
    },
    "client": {
      "autoReconnect": true,
      "timeout": 1000,
      "queueIncoming": false,
      "queueOutgoing": true
    }
  },
  "yandlr": {
    "loggers": {
      "wspubsub": {
        "console": {
          "level": "debug",
          "colorize": true
        }
      },
      "wspubsub:lib/Channel": {
        "console": {
          "level": "info",
          "colorize": true
        }
      }
    }
  }
}
